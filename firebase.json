{
    "hosting": {
      "public": "dist",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "cleanUrls": true,
      "trailingSlash": false,
      "headers": [
        {
          "source": "**/*.@(jpg|jpeg|gif|png|webp|svg|js|css)",
          "headers": [{ "key": "Cache-Control", "value": "max-age=604800" }]
        }
      ]
    }
  }
  